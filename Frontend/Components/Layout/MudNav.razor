
<MudNavMenu>
    @foreach (var navLink in _navLinks)
    {
        <MudNavLink Href="@navLink">@navLink.ToUpper()</MudNavLink>
    }
</MudNavMenu>


@code {

    readonly List<string> _navLinks =
    [
        "counter",
        "weather",
        "games",
        "newGame",
        "mudCounter",
        "mudGames",
        "mudNewGame"
    ];

}